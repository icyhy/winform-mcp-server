{
  "mcpServers": {
    "local-server": {
      "type": "sse",
      "url": "http://localhost:3000/sse",
      "note": "本地MCP服务器",
      "enabled": true,
      "timeout": 30000,
      "retryCount": 3
    },
    "example-http-server": {
      "type": "http",
      "url": "http://localhost:8080/mcp",
      "note": "示例HTTP MCP服务器",
      "enabled": false,
      "timeout": 30000,
      "retryCount": 3,
      "headers": {
        "Authorization": "Bearer your-token-here",
        "Content-Type": "application/json"
      }
    },
    "example-process-server": {
      "type": "process",
      "command": "node",
      "args": ["server.js"],
      "cwd": "/path/to/mcp/server",
      "note": "示例进程MCP服务器",
      "enabled": false,
      "timeout": 30000,
      "retryCount": 3,
      "env": {
        "NODE_ENV": "production"
      }
    }
  }
}